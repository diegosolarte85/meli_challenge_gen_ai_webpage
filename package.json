{
  "name": "meli-challenge-gen-ai-webpage",
  "version": "1.0.0",
  "description": "MercadoLibre-inspired item detail page with frontend and backend",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["mercadolibre", "ecommerce", "react", "nodejs", "express"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
} 